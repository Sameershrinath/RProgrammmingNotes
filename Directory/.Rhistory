# Install and load ggplot2 package
install.packages("ggplot2")
library(ggplot2)
# Create some sample data
x <- 1:10
y <- x^2
# Basic Plotting with plot()
plot(x, y, main="Scatter Plot", xlab="X-axis", ylab="Y-axis")
# Create a data frame for ggplot2
df <- data.frame(x = 1:10, y = (1:10)^2)
# Use ggplot2 to create a basic scatter plot
ggplot(data = df, aes(x = x, y = y)) +
geom_point() +
ggtitle("Scatter Plot with ggplot2") +
xlab("X-axis") +
ylab("Y-axis")
# Customize the ggplot2 plot with colors and shapes
ggplot(data = df, aes(x = x, y = y)) +
geom_point(color = "blue", shape = 16) +
ggtitle("Customized Scatter Plot") +
xlab("X-axis") +
ylab("Y-axis")
# Add a line of best fit to the ggplot2 plot
ggplot(data = df, aes(x = x, y = y)) +
geom_point() +
geom_smooth(method = "lm", se = FALSE) +
ggtitle("Scatter Plot with Best Fit Line") +
xlab("X-axis") +
ylab("Y-axis")
# Faceting for subplots in ggplot2
df$group <- rep(c("A", "B"), each = 5)
ggplot(data = df, aes(x = x, y = y)) +
geom_point() +
facet_wrap(~group) +
ggtitle("Faceted Scatter Plot") +
xlab("X-axis") +
ylab("Y-axis")
# Save a ggplot2 plot to a file
p <- ggplot(data = df, aes(x = x, y = y)) +
geom_point() +
ggtitle("Scatter Plot to Save") +
xlab("X-axis") +
ylab("Y-axis")
ggsave("my_plot.png", plot = p)
